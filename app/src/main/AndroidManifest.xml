<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="id.ac.unsyiah.elektro.mobile.tesbutawarna" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".WelcomeSplash"
            android:label="@string/title_activity_welcome_splash" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MulaiActivity"
            android:label="@string/title_activity_mulai" >
        </activity>
        <activity
            android:name=".HasilActivity"
            android:label="@string/title_activity_hasil" >
        </activity>
        <activity
            android:name=".PetunjukActivity"
            android:label="@string/title_activity_petunjuk" >
        </activity>
        <activity
            android:name=".ArtikelActivity"
            android:label="@string/title_activity_artikel" >
        </activity>
        <activity
            android:name=".Soal2"
            android:label="@string/title_activity_soal2" >
        </activity>
        <activity
            android:name=".Soal3"
            android:label="@string/title_activity_soal3" >
        </activity>
    </application>

</manifest>
